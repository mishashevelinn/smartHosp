<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="70.0" prefWidth="600.0" styleClass="root" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.FXMLController">
   <children>
      <Button fx:id="addNurseButton" mnemonicParsing="false" onAction="#addNurseButtonHandler" prefHeight="40.0" prefWidth="150.0" styleClass="button1" stylesheets="@stylesheet.css" text="Add Nurse">
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin></Button>
      <Button fx:id="removeNurseButton" mnemonicParsing="false" onAction="#removeNurseButtonHandler" prefHeight="40.0" prefWidth="150.0" styleClass="button1" text="Remove Nurse">
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin></Button>
   </children>
</HBox>
