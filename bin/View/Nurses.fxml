<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="nursesTable" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="nursesTable" styleClass="root" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="conditionColumn" prefWidth="75.0" text="Condition" />
            <TableColumn fx:id="dateRegisteredColumn" prefWidth="75.0" text="Date Registered" />
         </columns>
      </TableView>
   </children>
</VBox>
